{
  "environments": [
    {
      "name": "prd",
      "configOverride": {
        "defaultSchema": "dataform",
        "defaultDatabase": "gglobo-geoip-hdg-prd",
        "vars": {
          "env": "prd"
        }
      },
      "gitRef": "master"
    },
    {
      "name": "dev",
      "configOverride": {
        "defaultSchema": "dataform",
        "defaultDatabase": "gglobo-geoip-hdg-dev",
        "vars": {
          "env": "dev"
        }
      },
      "gitRef": "dev"
    }
  ],
  "schedules": [
    {
      "name": "mainPipelinePrd",
      "cron": "30 3 * * *",
      "tags": ["daily"],
      "disable": "true"
    },
    {
      "name": "devDailySchedule",
      "cron": "0 2 * * *",
      "tags": ["daily"],
      "disabled": false
    }
  ]
}
